{
  "name": "Eavesjob",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "babel ./app -d build",
    "build:watch": "babel ./app -d build --watch",
    "build:client": "webpack --config ./webpack.prod.config.js/",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch",
    "build:prod": "npm run build && npm run build:client",
    "start": "npm run build:prod && pm2 delete all && NODE_ENV=production pm2 start ./build/server/index.js",
    "restart": "pm2 restart all",
    "start:dev": "parallelshell 'npm run build' 'npm run build:watch:client' 'nodemon ./build/server/index.js'"
  },
  "author": "Daan_Schouten",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "json-markup": "^1.1.0",
    "moment": "^2.21.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "webfontloader": "^1.6.28",
    "dotenv": "^5.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.9",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^4.0.0",
    "jest": "^20.0.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mocha": "^5.0.0",
    "nodemon": "^1.11.0",
    "parallelshell": "^3.0.1",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.19.1",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^0.6.2",
